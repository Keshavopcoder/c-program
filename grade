#include <iostream>
using namespace std;
int main() 
{
int marks;
cout<<"Enter marks out of 100";
cin>>marks;
if (marks>=80) 
{
cout<<"Grade A";
}
else if(marks>=60 ) 
{
cout<<"Grade B";
}
 else if(marks>=40)
{
cout<<"Grade C"; 
}
else if(marks>=20) 
{
cout<<"Grade D";
}
else if(marks>=0)
{
cout<<"FAIL";
}
return 0;
}
